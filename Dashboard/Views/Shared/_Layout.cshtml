@{
    // Just some comments    
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title | Thomas G. Carpenter Library | University of North Florida</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="David King, Jr.">

	@Styles.Render( 
		"~/Content/css")

	@Scripts.Render(
		"~/bundles/jquery",
		"~/bundles/jquery-plugins",
		"~/bundles/highcharts",
		"~/bundles/dataTables",
		"~/bundles/library")
</head>
<body>
	@*@Html.Partial("_SiteNav")*@
	@Html.Partial("_SiteHeader")

    <hr />

    <div id="body">
        <div class="container-fluid">
            <div class="row-fluid">
                @RenderBody()
            </div>
        </div>
    </div>

    <hr />

	@Html.Partial("_SiteFooter")

	@RenderSection("scripts", required: false)
	@Scripts.Render("~/bundles/bootstrap")
</body>
</html>